{
  "name": "filaforge/quick-links",
  "description": "Quick Bookmarks dashboard widget for Filament v4 (per-user)",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "filament/filament": "^4.0",
    "laravel/framework": "^12.0",
    "spatie/laravel-package-tools": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Filaforge\\QuickLinks\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Filaforge\\QuickLinks\\Providers\\QuickLinksServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}



