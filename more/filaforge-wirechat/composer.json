{
    "name": "filaforge/wirechat",
    "description": "Filaforge Wirechat integration for Filament v4",
    "type": "library",
    "license": "MIT",
    "keywords": ["Laravel", "Filament", "Plugin", "Wirechat"],
    "authors": [
        { "name": "Filaforge" }
    ],
    "require": {
        "php": ">=8.1",
        "filament/filament": "^4.0",
        "illuminate/support": "^12.0",
        "spatie/laravel-package-tools": "^1.15"


    },
    "suggest": {
        "namumakwembo/wirechat": "^1.0 â€” Optional. Install to render the chat UI inside the Filament page."
    },
    "autoload": {
        "psr-4": {
            "Filaforge\\Wirechat\\": "src/",
            "Namu\\WireChat\\": "upstream/src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filaforge\\Wirechat\\Providers\\WirechatServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}


