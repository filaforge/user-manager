{
    "name": "filaforge/system-widget",
    "description": "Filament v4 dashboard widget for real-time system monitoring",
    "type": "library",
    "license": "MIT",
    "keywords": ["Laravel", "Filament", "Platform"],
    "homepage": "https://github.com/filaforge",
    "authors": [
        { "name": "Filaforge" }
    ],
    "support": {
        "email": "filaforger@gmail.com",
        "source": "https://github.com/filaforge"
    },
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0",
        "illuminate/support": "^12.0",
        "spatie/laravel-package-tools": "^1.16",
        "symfony/process": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Filaforge\\SystemWidget\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filaforge\\SystemWidget\\Providers\\SystemMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
