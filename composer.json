{
  "name": "filaforge/system-packages",
  "description": "System Packages widget: list all Composer packages in a Filament table",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "filament/filament": "^4.0",
  "illuminate/support": "^12.0",
    "spatie/laravel-package-tools": "^1.16"
  },
  "autoload": {
    "psr-4": {
      "Filaforge\\SystemPackages\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Filaforge\\SystemPackages\\Providers\\SystemPackagesServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

