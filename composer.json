{
  "name": "filaforge/user-manager",
  "description": "User manager plugin: Manage users and generate fake users",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "filament/filament": "^4.0",
    "laravel/framework": "^12.0",
    "spatie/laravel-package-tools": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Filaforge\\UserManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Filaforge\\UserManager\\Providers\\UserManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
