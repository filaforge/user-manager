{
  "name": "filaforge/api-explorer",
  "description": "API Explorer plugin for testing API endpoints similar to Postman",
  "type": "library",
  "license": "MIT",
  "keywords": ["Laravel", "Filament", "Platform"],
  "homepage": "https://github.com/filaforge/api-explorer",
  "authors": [
    { "name": "Filaforge" }
  ],
  "support": {
    "email": "filaforger@gmail.com",
  "source": "https://github.com/filaforge/api-explorer"
  },
  "require": {
  "php": ">=8.1",
  "filament/filament": "^4.0",
  "illuminate/support": "^12.0",
  "guzzlehttp/guzzle": "^7.2",
  "spatie/laravel-package-tools": "^1.15"
  },
  "autoload": {
    "psr-4": {
      "Filaforge\\ApiExplorer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Filaforge\\ApiExplorer\\Providers\\ApiExplorerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
