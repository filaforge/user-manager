{
  "name": "filaforge/database-viewer",
  "description": "Database viewer plugin: Database viewer with table browser and data explorer",
  "type": "library",
  "license": "MIT",
  "keywords": ["Laravel", "Filament", "Platform"],
  "homepage": "https://github.com/filaforge/database-viewer",
  "authors": [
    { "name": "Filaforge" }
  ],
  "support": {
    "email": "filaforger@gmail.com",
  "source": "https://github.com/filaforge/database-viewer"
  },
  "require": {
  "php": ">=8.1",
  "filament/filament": "^4.0",
  "illuminate/support": "^12.0",
  "spatie/laravel-package-tools": "^1.15"
  },
  "autoload": {
    "psr-4": {
      "Filaforge\\DatabaseViewer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Filaforge\\DatabaseViewer\\Providers\\DatabaseViewerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
